{
  "name": "everylayout-astro",
  "author": "Mike Connor",
  "type": "module",
  "private": true,
  "version": "2.0.0",
  "imports": {
    "#*": "./src/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mconnor/everylayout-astro.git"
  },
  "keywords": [
    "vite",
    "eslint",
    "astro"
  ],
  "scripts": {
    "dev": "astro dev",
    "test": "vitest",
    "check-types": "tsc --noEmit",
    "coverage": "vitest run --coverage",
    "host": "astro dev --host",
    "start": "astro dev",
    "check": "astro check",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "format": "prettier  --write   --ignore-unknown .",
    "format-check": "prettier --check .",
    "stylelint": "stylelint \"src/**/*.{css,astro}\"",
    "sfix": "stylelint \"src/**/*.{css,astro}\" --fix",
    "inspect": "eslint . --inspect-config"
  },
  "dependencies": {
