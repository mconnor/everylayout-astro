    "@sentry/astro": "^8.53.0",
    "@webcomponents/template-shadowroot": "^0.2.1",
    "add": "^2.0.6",
    "astro": "^5.5.2",
    "date-fns": "^4.1.0",
    "nanostores": "^0.11.3",
    "open-props": "^1.7.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.6.0",
    "sharp": "^0.33.5",
    "vitest": "^2.1.8"
