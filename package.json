    "@eslint/js": "^9.19.0",
    "@eslint/markdown": "^6.2.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-markdown": "^2.0.2",
    "@types/eslint__js": "^8.42.3",
