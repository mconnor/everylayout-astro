    "jiti": "^2.4.2",
    "lightningcss": "^1.29.1",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.14.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-prettier": "^5.0.3",
    "turbo": "^2.4.4",
    "typescript": "^5.7.3",
